{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "command_extractor",
    "version": "${version}",
    "metadata": {
      "name": "Command extractor",
      "description": "A Minecraft mod to extract available server commands to a JSON file.",
      "authors": [
        "IoeCmcomc"
      ],
      "contact": {
        "issues": "https://github.com/IoeCmcomc/command_extractor/issues",
        "sources": "https://github.com/IoeCmcomc/command_extractor"
      },
      "license": "MIT",
      "icon": "command_extractor_icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "ioecmcomc.command_extractor.quilt.CommandExtractorQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.21.1"
      },
      {
        "id": "architectury",
        "version": ">=13.0.8"
      }
    ]
  }
}